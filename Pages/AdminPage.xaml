<Page x:Class="EventManagement.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EventManagement.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Администратор">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Добавить пользователя" Width="200" Click="Button_AddUser"/>
        <Button Content="Все пользователи " Width="200" Click="Button_AllUsers"/>
        <Button Content="Отчет" Width="200" Click="Button_Click"/>
    </StackPanel>

    
</Page>
