<Page x:Class="EventManagement.Pages.OrganizerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EventManagement.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Организатор">
    <StackPanel Orientation="Vertical" VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button Content="Добавить мероприятие" Width="200" Click="Button_AddEvent"/>
        <Button Content="Все мероприятия " Width="200" Click="Button_AllEvents"/>
        <Button Content="Отчет" Width="200" Click="Button_Statistic"/>
    </StackPanel>
</Page>
